%Zoom an Image

%% start of the MATLAB code
clc 
close all 
clear 

%% Functional Code
%Read the image, give location to zoom inside 
%the array change it to double precision
Tesla=imread('Tesla.png');
Tesla_double=double(Tesla(250:317,200:280));

%Interpolate
b=interp2(Tesla_double,5);
subplot(2,1,1)
imshow(Tesla_double,[])
title('Before Interpolation')
subplot(2,1,2)
imshow(b,[])
title('After Interpolation')
%End of the MATLAB code